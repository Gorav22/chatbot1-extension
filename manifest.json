{
    "manifest_version": 3,
    "name": "Chatbot",
    "version": "1.0",
    "description": "A chatbot to give solution of your problem.",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "Godcode.png",
        "48": "Godcode.png",
        "128": "Godcode.png"
      }
    },
    "icons": {
      "16": "Godcode.png",
      "48": "Godcode.png",
      "128": "Godcode.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
  